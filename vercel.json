{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "functions": {
        "handlers/clientHandler.js": {
            "runtime": "nodejs14.x",
            "includeFiles": "dist/**"
        }
    }
}